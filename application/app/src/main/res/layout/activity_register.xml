<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha='1'
        android:scaleType="fitXY"
        android:src="@drawable/background1"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edTxtreg_confirm_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="105dp"
        android:backgroundTint="#0B0A0A"
        android:ems="10"
        android:hint="Confirm Password"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:layout_constraintBottom_toTopOf="@+id/edTxtreg_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edTxtreg_password" />

    <EditText
        android:id="@+id/edTxtreg_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="105dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:layout_constraintBottom_toTopOf="@+id/edTxtreg_phone_number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edTxtreg_confirm_password" />

    <EditText
        android:id="@+id/edTxtreg_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="105dp"
        android:backgroundTint="#040303"
        android:drawableTint="#0E0E0E"
        android:ems="10"
        android:hint="Password"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:layout_constraintBottom_toTopOf="@+id/edTxtreg_confirm_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edTxtreg_username" />

    <TextView
        android:id="@+id/txtReg"
        android:layout_width="450dp"
        android:layout_height="59dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="sans-serif-black"
        android:text="REGISTER ACCOUNT"
        android:textAlignment="center"
        android:textColor="#101010"
        android:textColorHighlight="#151515"
        android:textColorLink="#151414"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/edTxtreg_username"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edTxtreg_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="97dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="14dp"
        android:ems="10"
        android:hint="Username"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:layout_constraintBottom_toTopOf="@+id/edTxtreg_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtReg" />

    <EditText
        android:id="@+id/edTxtreg_phone_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="105dp"
        android:backgroundTint="#1B1919"
        android:ems="10"
        android:hint="Phone Number"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:layout_constraintBottom_toTopOf="@+id/btnReg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edTxtreg_email" />

    <Button
        android:id="@+id/btnReg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="155dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="153dp"
        android:backgroundTint="#dfd6d6"
        android:text="SIGN UP"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edTxtreg_phone_number" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="34dp"
        android:layout_height="40dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/edTxtreg_email"
        app:layout_constraintEnd_toStartOf="@+id/edTxtreg_email"
        app:layout_constraintTop_toTopOf="@+id/edTxtreg_email"
        app:srcCompat="@drawable/mail1" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="35dp"
        android:layout_height="42dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/edTxtreg_username"
        app:layout_constraintEnd_toStartOf="@+id/edTxtreg_username"
        app:layout_constraintTop_toBottomOf="@+id/txtReg"
        app:layout_constraintVertical_bias="0.921"
        app:srcCompat="@drawable/anonymous_empty_pfp3" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="35dp"
        android:layout_height="42dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/edTxtreg_confirm_password"
        app:layout_constraintEnd_toStartOf="@+id/edTxtreg_confirm_password"
        app:layout_constraintTop_toTopOf="@+id/edTxtreg_confirm_password"
        app:srcCompat="@drawable/approved" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="35dp"
        android:layout_height="42dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/edTxtreg_password"
        app:layout_constraintEnd_toStartOf="@+id/edTxtreg_password"
        app:layout_constraintTop_toTopOf="@+id/edTxtreg_password"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/lock1" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="35dp"
        android:layout_height="42dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/edTxtreg_phone_number"
        app:layout_constraintEnd_toStartOf="@+id/edTxtreg_phone_number"
        app:layout_constraintTop_toTopOf="@+id/edTxtreg_phone_number"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/phone_call3" />
</androidx.constraintlayout.widget.ConstraintLayout>